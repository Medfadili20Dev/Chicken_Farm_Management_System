@startuml Diagram_1_Domain_Model

title Diagram 1: Domain Model (Entities + Enums)

!define ENUM enum

' ========================================
' ENUMERATIONS
' ========================================

ENUM HouseType {
  CHICK_NURSERY
  EGG_LAYER
  MEAT_FEMALE
  MEAT_MALE
}

ENUM ChickenType {
  DAY_OLD_CHICK
  FEMALE_LAYER
  FEMALE_MEAT
  MALE_MEAT
}

ENUM HealthStatus {
  GOOD
  FAIR
  POOR
}

ENUM MortalityCause {
  DISEASE
  INJURY
  UNKNOWN
  NATURAL
}

ENUM FeedType {
  CHICK_STARTER
  EGG_LAYER
  MEAT_GROWER
}

ENUM EquipmentStatus {
  OPERATIONAL
  NEEDS_REPAIR
  BROKEN
}

ENUM TaskStatus {
  DONE
  PENDING
  MISSED
}

ENUM PersonnelRole {
  WORKER
  TRACKER
}

' ========================================
' MODEL CLASSES (Domain Entities)
' ========================================

class User {
  - userID: String
  - email: String
  - password: String
  - role: String
  - lastLogin: LocalDateTime
  --
  + User()
  + User(email, password, role)
  --
  + getUserID(): String
  + getEmail(): String
  + getPassword(): String
  + getRole(): String
  + getLastLogin(): LocalDateTime
  --
  + setUserID(userID: String): void
  + setEmail(email: String): void
  + setPassword(password: String): void
  + setRole(role: String): void
  + setLastLogin(lastLogin: LocalDateTime): void
}

class House {
  - houseID: String
  - houseNumber: int
  - houseType: HouseType
  - capacity: int
  - currentCount: int
  - healthStatus: HealthStatus
  - lastCleaningDate: LocalDate
  --
  + House()
  + House(houseNumber, houseType, capacity)
  --
  + getHouseID(): String
  + getHouseNumber(): int
  + getHouseType(): HouseType
  + getCapacity(): int
  + getCurrentCount(): int
  + getHealthStatus(): HealthStatus
  + getLastCleaningDate(): LocalDate
  --
  + setHouseID(houseID: String): void
  + setHouseNumber(houseNumber: int): void
  + setHouseType(houseType: HouseType): void
  + setCapacity(capacity: int): void
  + setCurrentCount(currentCount: int): void
  + setHealthStatus(healthStatus: HealthStatus): void
  + setLastCleaningDate(date: LocalDate): void
  --
  + calculateOccupancy(): double
  + getDaysUntilTransfer(): int
}

class Chicken {
  - chickenID: String
  - chickenType: ChickenType
  - gender: String
  - arrivalDate: LocalDate
  - houseID: String
  - healthStatus: HealthStatus
  - ageInDays: int
  --
  + Chicken()
  + Chicken(chickenType, gender, arrivalDate, houseID)
  --
  + getChickenID(): String
  + getChickenType(): ChickenType
  + getGender(): String
  + getArrivalDate(): LocalDate
  + getHouseID(): String
  + getHealthStatus(): HealthStatus
  + getAgeInDays(): int
  --
  + setChickenID(chickenID: String): void
  + setChickenType(chickenType: ChickenType): void
  + setGender(gender: String): void
  + setArrivalDate(arrivalDate: LocalDate): void
  + setHouseID(houseID: String): void
  + setHealthStatus(healthStatus: HealthStatus): void
  + setAgeInDays(ageInDays: int): void
  --
  + calculateAge(): int
  + shouldTransfer(): boolean
}

class Mortality {
  - mortalityID: String
  - chickenID: String
  - houseID: String
  - deathDate: LocalDate
  - cause: MortalityCause
  - count: int
  - notes: String
  --
  + Mortality()
  + Mortality(chickenID, houseID, deathDate, cause, count)
  --
  + getMortalityID(): String
  + getChickenID(): String
  + getHouseID(): String
  + getDeathDate(): LocalDate
  + getCause(): MortalityCause
  + getCount(): int
  + getNotes(): String
  --
  + setMortalityID(mortalityID: String): void
  + setChickenID(chickenID: String): void
  + setHouseID(houseID: String): void
  + setDeathDate(deathDate: LocalDate): void
  + setCause(cause: MortalityCause): void
  + setCount(count: int): void
  + setNotes(notes: String): void
}

class EggProduction {
  - productionID: String
  - houseID: String
  - collectionDate: LocalDate
  - eggsCollected: int
  - crackedEggs: int
  - deadChickens: int
  --
  + EggProduction()
