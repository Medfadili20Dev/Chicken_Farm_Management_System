  + setProductionID(productionID: String): void
  + setHouseID(houseID: String): void
  + setCollectionDate(collectionDate: LocalDate): void
  + setEggsCollected(eggsCollected: int): void
  + setCrackedEggs(crackedEggs: int): void
  + setDeadChickens(deadChickens: int): void
  --
  + calculateCleanEggs(): int
}

class Feed {
  - feedID: String
  - feedType: FeedType
  - quantityKg: double
  - lastUpdated: LocalDate
  - minThreshold: double
  --
  + Feed()
  + Feed(feedType, quantityKg, minThreshold)
  --
  + getFeedID(): String
  + getFeedType(): FeedType
  + getQuantityKg(): double
  + getLastUpdated(): LocalDate
  + getMinThreshold(): double
  --
  + setFeedID(feedID: String): void
  + setFeedType(feedType: FeedType): void
  + setQuantityKg(quantityKg: double): void
  + setLastUpdated(lastUpdated: LocalDate): void
  + setMinThreshold(minThreshold: double): void
