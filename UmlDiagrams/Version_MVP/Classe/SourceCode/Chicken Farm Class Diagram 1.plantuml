  + setEmail(email: String): void
  + setRole(role: PersonnelRole): void
}

' ========================================
' ENTITY RELATIONSHIPS
' ========================================

House "1" -- "0..*" Chicken : contains
House "1" -- "0..*" EggProduction : produces
House "1" -- "0..*" Mortality : records
Task "0..*" -- "0..1" Personnel : assigned_to

' ========================================
' ENUM RELATIONSHIPS
' ========================================

Chicken "*" --> "1" ChickenType
Chicken "*" --> "1" HealthStatus
House "*" --> "1" HouseType
House "*" --> "1" HealthStatus
Mortality "*" --> "1" MortalityCause
Feed "*" --> "1" FeedType
Equipment "*" --> "1" EquipmentStatus
Task "*" --> "1" TaskStatus
Personnel "*" --> "1" PersonnelRole


@enduml