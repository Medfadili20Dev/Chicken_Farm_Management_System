@startuml Chicken Farm Class Diagram

' ========================================
' CORE ENTITY CLASSES (8)
' ========================================

abstract class Supplier {
    +string supplierID
    +string companyName
    +string ice
    +string rc
    +string contactPerson
    +string phone
    +string email
    +string address
    +string bankDetails
    +string[] productCategories
    +string supplierType
}

class FeedSupplier {
    +string feedType
    +string[] certifications
}

class ChickSupplier {
    +string breedSpecialty
    +string hatcheryLocation
}

class MedicationSupplier {
    +string[] certifications
    +boolean prescriptionRequired
}

class EquipmentSupplier {
    +string[] equipmentTypes
    +boolean providesInstallation
    +boolean providesMaintenance
}

abstract class Customer {
    +string customerID
    +string name
    +string phone
    +string email
    +string address
    +string customerType
}

class CompanyCustomer {
    +string companyName
    +string ice
    +string rc
    +string contactPerson
    +string bankDetails
    +int paymentTerms
    +string[] usualPurchases
}

class IndividualCustomer {
    +string fullName
    +string cin
    +int age
    +boolean isRegular
}

abstract class Worker {
    +string workerID
    +string cin
    +string fullName
    +string address
    +string phone
    +string email
    +string emergencyContact
    +Date birthDate
    +Date startDate
    +Date endDate
    +string position
    +float monthlySalary
    +string cnssNumber
    +string status
    +string workerType
}

class Veterinary {
    +string licenseNumber
    +string[] specializations
    +Date licenseExpiryDate
}

class InventorySupplyTracker {
    +string trackingCertification
    +string[] managedCategories
}

class FarmhandSupervisor {
    +int subordinateCount
    +string[] responsibilities
    +string shiftSchedule
}

class Farmhand {
    +string assignedTasks
    +string supervisorID
    +string shiftSchedule
}

class Cashier {
    +string cashierID
    +string employeeID
    +string workstation
    +string[] assignedKeys
    +Date hireDate
}

class Administration {
    +string adminID
    +string department
    +string[] permissions
    +string decisionLevel
    +Date appointmentDate
}

class ChickenBatch {
    +string batchID
    +Date arrivalDate
    +int initialCount
    +int currentCount
    +int deadCount
    +string gender
    +int currentAge
    +string currentHouse
    +string supplierID
    +float mortalityRate
    +string batchStatus
}

class House {
    +string houseID
    +int houseNumber
    +int maxCapacity
    +int currentCapacity
    +string status
    +Date lastCleaningDate
    +string purpose
    +float temperature
    +float humidity
}

abstract class Product {
    +string productID
    +string productName
    +string productType
    +float price
    +string unit
    +float weight
    +string description
}

class Egg {
    +Date productionDate
    +string qualityStatus
    +string size
    +string grade
}

class Feed {
    +string feedType
    +string ageGroup
    +float proteinContent
    +Date manufactureDate
    +Date expiryDate
}

class Medication {
    +string medicationType
    +string dosage
    +Date expiryDate
    +string prescription
    +string activeIngredient
    +int withdrawalPeriod
}

class Equipment {
    +string equipmentType
    +string condition
    +Date purchaseDate
    +Date lastMaintenanceDate
    +int warrantyMonths
    +string manufacturer
}

class Chicken {
    +string chickenType
    +string gender
    +float weight
    +int age
    +string healthStatus
}

' ========================================
' DOCUMENT CLASSES (3)
' ========================================

class CommercialDocument {
    +string docID
    +string docType
    +string docNumber
    +Date issueDate
    +float amount
    +string status
    +string pdfPath
    +Date paymentDueDate
    +string sellerICE
    +string buyerICE
    +string notes
}

class EmploymentDocument {
    +string docID
    +string docType
    +Date issueDate
    +string content
    +string pdfPath
    +Date signatureDate
    +string month
    +string workerID
    +string signedBy
}

class LegalDocument {
    +string docID
    +string docType
    +string authority
    +Date issueDate
    +Date expiryDate
    +string documentNumber
    +string pdfPath
    +string description
    +string companyID
}

' ========================================
' TRANSACTION CLASSES (4)
' ========================================

class Order {
    +string orderID
    +string orderType
    +Date orderDate
    +string status
    +float totalAmount
    +Date deliveryDate
    +int paymentTerms
    +string supplierID
    +string customerID
    +string notes
}

class Payment {
    +string paymentID
    +string paymentType
    +float amount
    +string method
    +Date paymentDate
    +Date invoiceDate
    +string referenceNumber
    +string paidBy
    +string receivedBy
}

class Delivery {
    +string deliveryID
    +Date deliveryDate
    +string status
    +string inspectionNotes
    +string approvedBy
    +string driverName
    +string vehicleNumber
    +Date inspectionDate
}

class Sale {
    +string saleID
    +Date saleDate
    +string customerType
    +string customerID
    +float totalAmount
    +string paymentStatus
    +string receiptNumber
    +string handledBy
}

' ========================================
' TRACKING & RECORDS CLASSES (8)
' ========================================

class Inventory {
    +string inventoryID
    +string itemType
    +string productID
    +float quantity
    +string unit
    +Date lastUpdated
    +float minThreshold
    +float unitPrice
    +Date expirationDate
    +string storageLocation
}

class AttendanceRecord {
    +string recordID
    +string workerID
    +string scanType
    +DateTime timestamp
    +Date date
    +string qrCodeData
    +string location
}

class HealthRecord {
    +string recordID
    +string recordType
    +Date date
    +string chickenBatchID
    +string details
    +string veterinaryNotes
    +string symptoms
    +string treatment
    +float temperature
    +float humidity
    +string veterinaryID
}

class ProductionRecord {
    +string recordID
    +string recordType
    +Date date
    +float quantity
    +string houseID
    +string batchID
    +string notes
    +int crackedEggs
    +int cleanEggs
    +string recordedBy
}

class FinancialRecord {
    +string recordID
    +string recordType
    +float amount
    +string category
    +Date date
    +string description
    +float cashReceived
    +float cashPaid
    +float balance
    +float discrepancy
    +string recordedBy
}

class WorkerSalary {
    +string salaryID
    +string workerID
    +string month
    +float baseSalary
    +int daysWorked
    +float cnssDeduction
    +float amoDeduction
    +float incomeTax
    +float netSalary
    +float overtime
    +float bonuses
    +Date paymentDate
}

class Agreement {
    +string agreementID
    +string agreementType
    +string supplierID
    +string customerID
    +string terms
    +Date startDate
    +Date endDate
    +int paymentDelay
    +string returnPolicy
    +float minimumOrder
    +Date renewalDate
}

class InspectionRecord {
    +string inspectionID
    +string inspectionType
    +Date date
    +string inspector
    +string findings
    +string status
    +string recommendations
    +int houseNumber
    +string issuesFound
}

' ========================================
' SYSTEM CLASSES (5)
' ========================================

class Report {
    +string reportID
    +string reportType
    +Date generationDate
    +string period
    +string data
    +string pdfPath
    +Date periodStart
    +Date periodEnd
    +string generatedBy
}

class Alert {
    +string alertID
    +string alertType
    +string priority
    +string message
    +DateTime timestamp
    +string status
    +string targetUser
    +boolean isResolved
    +Date resolvedDate
}

class SystemConfiguration {
    +string configID
    +string farmName
    +string theme
    +string settings
    +Date lastModified
    +string language
    +string dateFormat
    +string timeFormat
    +boolean autoBackupEnabled
    +string backupFrequency
}

class User {
    +string userID
    +string username
    +string password
    +string role
    +string qrCode
    +string[] permissions
    +Date lastLogin
    +string email
    +string phone
    +boolean isActive
}

class Task {
    +string taskID
    +string taskName
    +string taskType
    +string priority
    +string status
    +string assignedRole
    +Date dueDate
    +string dueTime
    +Date completedDate
    +string notes
    +boolean isRecurring
    +string createdBy
}

' ========================================
' SUPPORTING CLASSES (3)
' ========================================

class QuotationMatrix {
    +string matrixID
    +string supplierID
    +float costScore
    +float qualityScore
    +float delayScore
    +float totalScore
    +Date evaluationDate
    +string orderID
    +string evaluatedBy
}

class Company {
    +string companyID
    +string ice
    +string rc
    +string taxID
    +string cnssNumber
    +string onssaAuthorization
    +string bankDetails
    +string companyName
    +string address
    +Date registrationDate
    +float shareCapital
    +string legalForm
    +string businessField
}

class OrderLine {
    +string orderLineID
    +string orderID
    +string productID
    +int quantity
    +float unitPrice
    +float totalPrice
    +string notes
}

class SaleLine {
    +string saleLineID
    +string saleID
    +string productID
    +int quantity
    +float unitPrice
    +float totalPrice
    +string notes
}

' ========================================
' INHERITANCE RELATIONSHIPS
' ========================================

Supplier <|-- FeedSupplier
Supplier <|-- ChickSupplier
Supplier <|-- MedicationSupplier
Supplier <|-- EquipmentSupplier

Customer <|-- CompanyCustomer
Customer <|-- IndividualCustomer

Worker <|-- Veterinary
Worker <|-- InventorySupplyTracker
Worker <|-- FarmhandSupervisor
Worker <|-- Farmhand

Product <|-- Egg
Product <|-- Feed
Product <|-- Medication
Product <|-- Equipment
Product <|-- Chicken

' ========================================
' COMMERCIAL RELATIONSHIPS
' ========================================

Supplier "1" -- "*" Order : supplies
Customer "1" -- "*" Order : places
Supplier "1" -- "0..1" Agreement : signs
Customer "1" -- "0..1" Agreement : signs
Order "1" -- "1..*" CommercialDocument : generates
Order "1" -- "0..1" Delivery : results_in
Delivery "1" -- "1..*" CommercialDocument : documented_by
Customer "1" -- "*" Sale : makes
Sale "1" -- "1..*" CommercialDocument : documented_by

' ========================================
' PAYMENT RELATIONSHIPS
' ========================================

Supplier "1" -- "*" Payment : receives
Customer "1" -- "*" Payment : makes
Worker "1" -- "*" Payment : receives
CommercialDocument "1" -- "0..1" Payment : triggers
Payment "1" -- "1" FinancialRecord : recorded_in

' ========================================
' WORKER & EMPLOYMENT RELATIONSHIPS
' ========================================

Worker "1" -- "*" EmploymentDocument : has
Worker "1" -- "*" AttendanceRecord : records
Worker "1" -- "*" WorkerSalary : earns
AttendanceRecord "*" -- "1" WorkerSalary : calculates
WorkerSalary "1" -- "1" EmploymentDocument : generates
Administration "1" -- "*" Worker : manages
Cashier "1" -- "*" Worker : tracks
Worker "1" -- "0..1" User : has_account
Cashier "1" -- "1" User : has_account
Administration "1" -- "1" User : has_account

' ========================================
' CHICKEN & PRODUCTION RELATIONSHIPS
' ========================================

House "1" -- "0..1" ChickenBatch : contains
Supplier "1" -- "*" ChickenBatch : provides
ChickenBatch "1" -- "*" ProductionRecord : tracked_by
ChickenBatch "1" -- "*" HealthRecord : monitored_by
House "1" -- "*" InspectionRecord : inspected_by
Veterinary "1" -- "*" HealthRecord : creates
FarmhandSupervisor "1" -- "*" InspectionRecord : creates

' ========================================
' INVENTORY & PRODUCT RELATIONSHIPS
' ========================================

Product "1" -- "1" Inventory : tracked_in
InventorySupplyTracker "1" -- "*" Inventory : manages
Supplier "1" -- "*" Product : provides
Order "*" -- "*" Product : contains
Sale "*" -- "*" Product : contains
Inventory "1" -- "*" Alert : triggers
InventorySupplyTracker "1" -- "*" ProductionRecord : records

' ========================================
' FINANCIAL RELATIONSHIPS
' ========================================

Cashier "1" -- "*" FinancialRecord : maintains
Order "1" -- "1..*" FinancialRecord : generates
Sale "1" -- "1" FinancialRecord : generates
WorkerSalary "1" -- "1" FinancialRecord : generates

' ========================================
' TASK MANAGEMENT RELATIONSHIPS
' ========================================

Cashier "1" -- "*" Task : performs
Worker "1" -- "*" Task : assigned_to
Veterinary "1" -- "*" Task : performs
InventorySupplyTracker "1" -- "*" Task : performs
FarmhandSupervisor "1" -- "*" Task : performs
Administration "1" -- "*" Task : approves
HealthRecord "1" -- "0..1" Task : created_by
ProductionRecord "1" -- "0..1" Task : created_by
InspectionRecord "1" -- "0..1" Task : created_by
AttendanceRecord "1" -- "0..1" Task : created_by
FinancialRecord "1" -- "0..1" Task : created_by
Alert "1" -- "0..1" Task : creates

' ========================================
' SYSTEM & REPORTING RELATIONSHIPS
' ========================================

ProductionRecord "*" -- "1" Report : compiled_in
FinancialRecord "*" -- "1" Report : compiled_in
HealthRecord "*" -- "1" Report : compiled_in
Inventory "*" -- "1" Report : compiled_in
Task "*" -- "1" Report : compiled_in
Cashier "1" -- "*" Report : generates
Cashier "1" -- "*" Alert : receives
Administration "1" -- "*" Alert : receives
HealthRecord "1" -- "0..*" Alert : triggers
ChickenBatch "1" -- "*" Alert : triggers

' ========================================
' LEGAL & COMPLIANCE RELATIONSHIPS
' ========================================

Company "1" -- "*" LegalDocument : owns
Company "1" -- "*" Supplier : purchases_from
Company "1" -- "*" Customer : sells_to
Company "1" -- "*" Worker : employs
Company "1" -- "1" SystemConfiguration : configures
Supplier "1" -- "*" QuotationMatrix : evaluated_by
Order "1" -- "0..1" QuotationMatrix : uses

' ========================================
' COMPOSITION RELATIONSHIPS (Strong ownership)
' ========================================

Company *-- "1" SystemConfiguration
Order *-- "1..*" CommercialDocument
Sale *-- "1..*" CommercialDocument
Worker *-- "*" EmploymentDocument
ChickenBatch *-- "*" ProductionRecord
ChickenBatch *-- "*" HealthRecord
Task *-- "*" HealthRecord
Task *-- "*" ProductionRecord
Task *-- "*" InspectionRecord

' ========================================
' AGGREGATION RELATIONSHIPS (Weak ownership)
' ========================================

House o-- "0..1" ChickenBatch
Report o-- "*" ProductionRecord
Report o-- "*" FinancialRecord
Report o-- "*" HealthRecord
Report o-- "*" Task

' ========================================
' ASSOCIATION CLASSES
' ========================================

Order .. OrderLine
Product .. OrderLine
Sale .. SaleLine
Product .. SaleLine

@enduml